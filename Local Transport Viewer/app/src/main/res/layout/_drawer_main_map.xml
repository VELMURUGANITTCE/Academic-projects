<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/full_layer"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="8dp"
		android:orientation="horizontal">
		<LinearLayout
			android:id="@+id/user_details_linear"
			android:layout_width="250dp"
			android:layout_height="50dp"
			android:orientation="horizontal"
			android:translationX="5.0dp"
			android:translationY="15.0dp">
			<LinearLayout
				android:id="@+id/user_logo_linear"
				android:layout_width="50dp"
				android:layout_height="50dp"
				android:padding="8dp"
				android:background="@drawable/user_logo"
				android:orientation="vertical"/>
			<LinearLayout
				android:id="@+id/linear18"
				android:layout_width="match_parent"
				android:layout_height="50dp"
				android:paddingLeft="8dp"
				android:paddingRight="8dp"
				android:gravity="left|center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview8"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:textSize="18sp"
					android:textStyle="bold"
					android:textColor="#42A5F5"/>
				<TextView
					android:id="@+id/name"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:textSize="18sp"
					android:textColor="#000000"/>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/cancel_logo_linear"
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:padding="8dp"
			android:background="@drawable/cancel_logo"
			android:orientation="horizontal"
			android:translationX="5.0dp"
			android:translationY="15.0dp"/>
		<LinearLayout
			android:id="@+id/selecting_linear"
			android:layout_width="315dp"
			android:layout_height="550dp"
			android:padding="8dp"
			android:gravity="center_horizontal"
			android:orientation="horizontal"
			android:translationX="-260.0dp"
			android:translationY="100.0dp">
			<LinearLayout
				android:id="@+id/title_linear"
				android:layout_width="300dp"
				android:layout_height="50dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal"
				android:translationX="740.0dp">
				<TextView
					android:id="@+id/textview1"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingTop="8dp"
					android:paddingRight="8dp"
					android:paddingBottom="8dp"
					android:text="SELECT THE VEHICLES"
					android:textSize="25sp"
					android:textStyle="bold"
					android:textColor="#000000"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/bike_linear"
				android:layout_width="300dp"
				android:layout_height="50dp"
				android:paddingLeft="8dp"
				android:paddingRight="8dp"
				android:orientation="horizontal"
				android:translationX="450.0dp"
				android:translationY="100.0dp">
				<LinearLayout
					android:id="@+id/linear2"
					android:layout_width="50dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:background="@drawable/bike_logo3"
					android:orientation="vertical"/>
				<LinearLayout
					android:id="@+id/linear8"
					android:layout_width="150dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview2"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingRight="8dp"
						android:text="BIKE"
						android:textSize="25sp"
						android:textColor="#000000"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear13"
					android:layout_width="80dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:orientation="vertical">
					<Switch
						android:id="@+id/bike_switch"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:textSize="12sp"
						android:textColor="#000000"/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/car_linear"
				android:layout_width="300dp"
				android:layout_height="50dp"
				android:paddingLeft="8dp"
				android:paddingRight="8dp"
				android:orientation="horizontal"
				android:translationX="150.0dp"
				android:translationY="175.0dp">
				<LinearLayout
					android:id="@+id/linear4"
					android:layout_width="50dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:background="@drawable/car_logo1"
					android:orientation="vertical"/>
				<LinearLayout
					android:id="@+id/linear9"
					android:layout_width="150dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview4"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingRight="8dp"
						android:text="CAR"
						android:textSize="25sp"
						android:textColor="#000000"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear14"
					android:layout_width="80dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:orientation="vertical">
					<Switch
						android:id="@+id/car_switch"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:textSize="12sp"
						android:textColor="#000000"/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/auto_linear"
				android:layout_width="300dp"
				android:layout_height="50dp"
				android:paddingLeft="8dp"
				android:paddingRight="8dp"
				android:orientation="horizontal"
				android:translationX="-150.0dp"
				android:translationY="250.0dp">
				<LinearLayout
					android:id="@+id/linear5"
					android:layout_width="50dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:background="@drawable/auto_logo1"
					android:orientation="vertical"/>
				<LinearLayout
					android:id="@+id/linear10"
					android:layout_width="150dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview5"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingRight="8dp"
						android:text="AUTO"
						android:textSize="25sp"
						android:textColor="#000000"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear15"
					android:layout_width="80dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:orientation="vertical">
					<Switch
						android:id="@+id/auto_switch"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:textSize="12sp"
						android:textColor="#000000"/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/bus_linear"
				android:layout_width="300dp"
				android:layout_height="50dp"
				android:paddingLeft="8dp"
				android:paddingRight="8dp"
				android:orientation="horizontal"
				android:translationX="-450.0dp"
				android:translationY="325.0dp">
				<LinearLayout
					android:id="@+id/linear6"
					android:layout_width="50dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:background="@drawable/bus_logo1"
					android:orientation="vertical"/>
				<LinearLayout
					android:id="@+id/linear11"
					android:layout_width="150dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview6"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingRight="8dp"
						android:text="BUS"
						android:textSize="25sp"
						android:textColor="#000000"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear16"
					android:layout_width="80dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:orientation="vertical">
					<Switch
						android:id="@+id/bus_switch"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:textSize="12sp"
						android:textColor="#000000"/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/lorry_linear"
				android:layout_width="300dp"
				android:layout_height="50dp"
				android:paddingLeft="8dp"
				android:paddingRight="8dp"
				android:orientation="horizontal"
				android:translationX="-750.0dp"
				android:translationY="400.0dp">
				<LinearLayout
					android:id="@+id/linear7"
					android:layout_width="50dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:background="@drawable/lorry_logo1"
					android:orientation="vertical"/>
				<LinearLayout
					android:id="@+id/linear12"
					android:layout_width="150dp"
					android:layout_height="match_parent"
					android:paddingLeft="8dp"
					android:paddingRight="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview7"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="LORRY"
						android:textSize="25sp"
						android:textColor="#000000"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear17"
					android:layout_width="80dp"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:orientation="vertical">
					<Switch
						android:id="@+id/lorry_switch"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:textSize="12sp"
						android:textColor="#000000"/>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
